<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.Ilmiddin1701.loyiha.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_lessonFragment"
            app:destination="@id/lessonFragment" />
    </fragment>
    <fragment
        android:id="@+id/lessonFragment"
        android:name="com.Ilmiddin1701.loyiha.fragments.LessonFragment"
        android:label="fragment_lesson"
        tools:layout="@layout/fragment_lesson" >
        <action
            android:id="@+id/action_lessonFragment_to_helpingFragment"
            app:destination="@id/helpingFragment" />
    </fragment>
    <fragment
        android:id="@+id/helpingFragment"
        android:name="com.Ilmiddin1701.loyiha.fragments.HelpingFragment"
        android:label="fragment_helping"
        tools:layout="@layout/fragment_helping" >
        <action
            android:id="@+id/action_helpingFragment_to_booksFragment"
            app:destination="@id/booksFragment" />
    </fragment>
    <fragment
        android:id="@+id/booksFragment"
        android:name="com.Ilmiddin1701.loyiha.fragments.BooksFragment"
        android:label="fragment_books"
        tools:layout="@layout/fragment_books" >
        <action
            android:id="@+id/action_booksFragment_to_informationFragment"
            app:destination="@id/informationFragment" />
    </fragment>
    <fragment
        android:id="@+id/informationFragment"
        android:name="com.Ilmiddin1701.loyiha.fragments.InformationFragment"
        android:label="fragment_information"
        tools:layout="@layout/fragment_information" >
        <action
            android:id="@+id/action_informationFragment_to_foodFragment"
            app:destination="@id/foodFragment" />
    </fragment>
    <fragment
        android:id="@+id/foodFragment"
        android:name="com.Ilmiddin1701.loyiha.fragments.FoodFragment"
        android:label="fragment_food"
        tools:layout="@layout/fragment_food" >
        <action
            android:id="@+id/action_foodFragment_to_youtubeFragment"
            app:destination="@id/youtubeFragment" />
    </fragment>
    <fragment
        android:id="@+id/youtubeFragment"
        android:name="com.Ilmiddin1701.loyiha.fragments.YoutubeFragment"
        android:label="fragment_youtube"
        tools:layout="@layout/fragment_youtube" >
        <action
            android:id="@+id/action_youtubeFragment_to_aboutMatherFragment"
            app:destination="@id/aboutMatherFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutMatherFragment"
        android:name="com.Ilmiddin1701.loyiha.fragments.AboutMatherFragment"
        android:label="fragment_about_mather"
        tools:layout="@layout/fragment_about_mather" />
</navigation>